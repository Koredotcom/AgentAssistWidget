{
  "name": "Kore.ai AgentAssist for Zendesk",
  "author": {
    "name": "KoreAI",
    "email": "koreai@kore.com",
    "url": "https://kore.ai/"
  },
  "defaultLocale": "en",
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html"
    }
  },
  "version": "1.0",
  "frameworkVersion": "2.0",
  "parameters": [
    {
      "name": "agentassistURL",
      "type": "url",
      "required":true
     
    },
    {
      "name": "stsURL",
      "type": "url",
      "required":true    
    },
    {
      "name":"domain",
      "type":"text",
      "required":true,
      "secure":true
    },
    {
      "name": "botClientId",
      "type": "password",
      "required":true,
      "secure":false    
    },
    {
      "name": "botClientSecret",
      "type": "password",
      "required":true,
      "secure": true

    },
    {
      "name": "botId",
      "type": "text",
      "required":true  
    }

  ],
  "domainWhiteList":["*.{{setting.domain}}"]
}

{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "AgentAssistWidget": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "baseHref": "/koreagentassist-sdk/",
            "deployUrl": "/koreagentassist-sdk/",
            "outputPath": "dist/AgentAssistWidget",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/reset.css",
              "src/styles.scss",
              "src/assets/fonts/AgentAssist-icons/style.css",
              "src/assets/css/jquery-ui.min.css",
              "src/assets/css/daterangepicker.css",
              "src/assets/css/jquery-clockpicker.css",
              "src/assets/css/kore-pickers.css",
              "src/assets/css/chatWindow.css",
              "src/assets/css/emojione.sprites.css",
              "src/assets/css/purejscarousel.css",
              "src/assets/css/customTemplate.css",
              "src/assets/css/adaptivecards.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/common/agent-assistv2/jquery-ui.min.js",
              "src/common/agent-assistv2/jquery.tmpl.min.js",
              "src/common/agent-assistv2/kore-bot-sdk-client.js",
              "src/common/agent-assistv2/emoji.js",
              "src/common/agent-assistv2/d3.v4.min.js",
              "src/common/agent-assistv2/purejscarousel.js",
              "src/common/agent-assistv2/agentAssistChatWindow.js",
              "src/common/agent-assistv2/agentAssistCustomTemplate.js",
              "src/common/agent-assistv2/KoreGraphAdapter.js",
              "src/common/agent-assistv2/socketConnection.js",
              "src/common/agent-assistv2/adaptivecards.js",
              "src/common/agent-assistv2/kore-adaptive-cards-plugin.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production": {

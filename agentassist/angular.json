              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",

              "src/app/scss/styles.scss",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/lato-font/css/lato-font.min.css",
              "src/app/helpers/lib/assets/highlight.js/agate.min.css",
              "src/assets/fonts/inter/inter-font.css",
              "node_modules/froala-editor/css/froala_editor.pkgd.min.css",
              "node_modules/froala-editor/css/froala_style.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",

              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/d3/dist/d3.min.js",
              "src/app/helpers/lib/assets/highlight.js/highlight.min.js",
              "src/app/helpers/lib/assets/marked.min.js",
              "src/assets/js/purejscarousel.js",
              "src/assets/js/graphAdapter.js",
              "src/assets/js/stringProto.js"

            ],
            "allowedCommonJsDependencies": [
              "echarts",
              "moment-timezone",
              "rxjs",
              "froala-editor"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
